	},
	"item": [
		{
			"name": "Pilot",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Extract the token from the GraphQL response",
							"let jsonData = pm.response.json();",
							"let token = jsonData.data.login.token;",
							"",
							"// Save token in the active environment (dev/prod)",
							"pm.globals.set(\"Token_Pilot\", token);",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "graphql",
					"graphql": {
						"query": "mutation Login {\n    login(\n        email:\"{{login_email_pilot}}\"\n        password: \"{{login_password_pilot}}\"\n    ) {\n        ... on LoginSuccess {\n            token\n        }\n    }\n}\n",
						"variables": ""
					}
				},
				"url": {
					"raw": "{{URL}}",
					"host": [
						"{{URL}}"
					]
				}
			},
			"response": []
		},
